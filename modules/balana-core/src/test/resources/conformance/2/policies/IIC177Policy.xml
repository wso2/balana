<?xml version="1.0" encoding="UTF-8"?>
<Policy
      xmlns="urn:oasis:names:tc:xacml:2.0:policy:schema:os"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="urn:oasis:names:tc:xacml:2.0:policy:schema:os
        access_control-xacml-2.0-policy-schema-os.xsd"
      PolicyId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIC177:policy"
      RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:deny-overrides">
    <Description>
        Policy for Conformance Test IIC177.
    </Description>
    <Target/>
    <Rule
          RuleId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIC177:rule"
          Effect="Permit">
        <Condition>
<Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:boolean-at-least-one-member-of">
            <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:boolean-bag">
                <AttributeValue
                      DataType="http://www.w3.org/2001/XMLSchema#boolean">false</AttributeValue>
                <AttributeValue
                      DataType="http://www.w3.org/2001/XMLSchema#boolean">true</AttributeValue>
            </Apply>
            <SubjectAttributeDesignator
                  AttributeId="urn:oasis:names:tc:xacml:2.0:conformance-test:test-attr"
                  DataType="http://www.w3.org/2001/XMLSchema#boolean"/>
        
</Apply></Condition>
    </Rule>
</Policy>
