<?xml version="1.0" encoding="UTF-8"?>
<Policy
      xmlns="urn:oasis:names:tc:xacml:2.0:policy:schema:os"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="urn:oasis:names:tc:xacml:2.0:policy:schema:os
        access_control-xacml-2.0-policy-schema-os.xsd"
      PolicyId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIC152:policy"
      RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:deny-overrides">
    <Description>
        Policy for Conformance Test IIC152.
    </Description>
    <Target/>
    <Rule
          RuleId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIC152:rule"
          Effect="Permit">
        <Condition>
<Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:dayTimeDuration-is-in">
            <AttributeValue
                  DataType="http://www.w3.org/TR/2002/WD-xquery-operators-20020816#dayTimeDuration">P50DT4H4M3S</AttributeValue>
            <SubjectAttributeDesignator
                  AttributeId="urn:oasis:names:tc:xacml:2.0:conformance-test:test-attr"
                  DataType="http://www.w3.org/TR/2002/WD-xquery-operators-20020816#dayTimeDuration"/>
        
</Apply></Condition>
    </Rule>
</Policy>
