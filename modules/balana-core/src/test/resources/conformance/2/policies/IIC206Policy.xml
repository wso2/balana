<?xml version="1.0" encoding="UTF-8"?>
<Policy
      xmlns="urn:oasis:names:tc:xacml:2.0:policy:schema:os"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="urn:oasis:names:tc:xacml:2.0:policy:schema:os
        access_control-xacml-2.0-policy-schema-os.xsd"
      PolicyId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIC206:policy"
      RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:deny-overrides">
    <Description>
        Policy for Conformance Test IIC206.
    </Description>
    <Target/>
    <Rule
          RuleId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIC206:rule"
          Effect="Permit">
        <Condition>
<Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:integer-equal">
            <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:anyURI-bag-size">
                <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:anyURI-intersection">
                    <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:anyURI-bag">
                        <AttributeValue
                              DataType="http://www.w3.org/2001/XMLSchema#anyURI">urn:oasis:names:tc:xacml:1.0:resource:resource-id</AttributeValue>
                        <AttributeValue
                              DataType="http://www.w3.org/2001/XMLSchema#anyURI">http://medico.com/record/patient/BartSimpson</AttributeValue>
                    </Apply>
                    <SubjectAttributeDesignator
                          AttributeId="urn:oasis:names:tc:xacml:2.0:conformance-test:test-attr"
                          DataType="http://www.w3.org/2001/XMLSchema#anyURI"/>
                </Apply>
            </Apply>
            <AttributeValue
                  DataType="http://www.w3.org/2001/XMLSchema#integer">2</AttributeValue>
        
</Apply></Condition>
    </Rule>
</Policy>
